{
    "manifest_version": 3,
    "name": "DuckDuckGo Proxy for Imgur",
    "version": "0.1",
    "description": "Use the DuckDuckGo proxy to load Imgur images",
    "permissions": [
        "declarativeNetRequest"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}